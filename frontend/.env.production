# Production Environment Configuration for Frontend

# API Configuration
VITE_API_URL=https://api.your-domain.com

# Environment
VITE_ENV=production
VITE_DEBUG=false

# Application Settings
VITE_APP_TITLE=Cryptocurrency Marketplace
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_SHOP_FEATURES=true

# Payment Gateway Configuration
VITE_PAYMENT_GATEWAY_API_KEY=your_payment_gateway_api_key

# Blockchain API Keys (for frontend transaction verification)
VITE_BLOCKCYPHER_TOKEN=your_blockcypher_token
VITE_ETHERSCAN_API_KEY=your_etherscan_api_key
VITE_BSCSCAN_API_KEY=your_bscscan_api_key

# Analytics
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_WEBSOCKETS=true

# Security
VITE_API_TIMEOUT=10000  # 10 seconds
